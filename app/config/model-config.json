{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "loopback-dummy-project/models",
      "@joinbox/loopback-component-jb-migration/src/models",
      "../models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../mixins"
    ]
  },
  "Migration": {
    "dataSource": "db",
    "public": false
  },
  "User": {
    "dataSource": "auth"
  },
  "AccessToken": {
    "dataSource": "auth",
    "public": false
  },
  "ACL": {
    "dataSource": "auth",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "auth",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "auth",
    "public": false
  },
  "Book": {
      "dataSource": "db",
      "public": true
  },
  "Page": {
      "dataSource": "db",
      "public": true
  },
  "Author": {
      "dataSource": "db",
      "public": true,
      "options": {
          "setCustomHeaders": true,
          "customHeaders": {
              "Cache-Control": "public, max-age=86400",
              "Vary": "User-Agent"
          }
      }
  },
  "Publisher": {
      "dataSource": "db",
      "public": true
  }
}
